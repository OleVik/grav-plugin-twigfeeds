{
  "config": {
    "preferred-install": "dist",
    "platform-check": false
  },
  "require": {
    "symfony/filesystem": "^3.2",
    "naneau/semver": "^0.0.7",
    "psr/log": "^1.1",
    "kevinrob/guzzle-cache-middleware": "^3.2",
    "php-feed-io/feed-io": "5.3.3"
  },
  "autoload": {
    "psr-4": {
      "Grav\\Plugin\\TwigFeedsPlugin\\": "classes/",
      "Grav\\Plugin\\TwigFeedsPlugin\\API\\": "classes/",
      "FeedIo\\Adapter\\Guzzle\\": "classes/FeedIo/Adapter/Guzzle"
    },
    "files": [
      "classes/FeedIo/Adapter/Guzzle/Client.php"
    ],
    "exclude-from-classmap": [
      "vendor/php-feed-io/feed-io/src/FeedIo/Adapter/Guzzle/Client.php"
    ]
  }
}
